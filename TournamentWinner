#include <vector>
using namespace std;

string tournamentWinner(vector<vector<string>> competitions, vector<int> results) 
{
  int res_index = 0;
  std::unordered_map<std::string, int> team_points;
  for(auto competition: competitions)
  {
    auto result = results[res_index];
    for(int i=0; i<competition.size();i++)
    {
        std::string competitor = competition[i]; 
        if(result == 1 && i==0)
          team_points[competitor] = team_points[competitor] + 3;
        if(result == 0 && i==1)
          team_points[competitor] = team_points[competitor] + 3;
    }
    res_index++;
  }
  int max =0;
  std::string max_team;
  for(auto team_point : team_points)
  {
     //std::cout<< team_point.first << ":" << team_point.second;
     if(team_point.second > max)
     {
      max=team_point.second;
      max_team = team_point.first;
     }  
  }
  return max_team;
}
